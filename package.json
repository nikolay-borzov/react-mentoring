{
  "name": "react-mentoring",
  "version": "0.0.1",
  "description": "React study project",
  "private": true,
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "start:analyze": "cross-env ANALIZE=true node server.js",
    "start:prod:analyze": "cross-env NODE_ENV=production ANALIZE=true node server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": ["eslint --fix", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolay-borzov/react-mentoring.git"
  },
  "keywords": [
    "study-project"
  ],
  "author": "Nikolay Borzov <nikolay.n.borzov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nikolay-borzov/react-mentoring/issues"
  },
  "homepage": "https://github.com/nikolay-borzov/react-mentoring#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-prettier-standard": "^1.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.1",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "npm-run-all": "^4.1.2",
    "opn": "^5.3.0",

  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-load-image": "^0.1.6",
    "react-redux": "^5.0.7",
    "react-required-if": "^1.0.3",
    "react-router": "^4.2.0",
    "react-toastify": "^4.0.1",
    "redux": "^4.0.0"
  }
}

{
  "name": "react-mentoring",
  "version": "0.3.0",
  "description": "React study project",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "node server.js",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "build": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "build:prod:analyze": "cross-env ANALYZE=true npm run build:prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolay-borzov/react-mentoring.git"
  },
  "keywords": [
    "study-project"
  ],
  "author": "Nikolay Borzov <nikolay.n.borzov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nikolay-borzov/react-mentoring/issues"
  },
  "homepage": "https://github.com/nikolay-borzov/react-mentoring#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.2",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "lodash-es": "^4.17.10",
    "npm-run-all": "^4.1.3",
    "opn": "^5.3.0",
    "postcss": "^6.0.22",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.12.1",
    "promise.prototype.finally": "^3.1.0",
    "react-hot-loader": "^4.1.2",
    "react-test-renderer": "^16.3.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-search-params-polyfill": "^4.0.0",
    "webpack": "^4.8.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.1.3",
